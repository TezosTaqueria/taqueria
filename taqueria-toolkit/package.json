{
	"name": "@taqueria/toolkit",
	"version": "0.28.7",
	"description": "A TypeScript library for NodeJS to work with Taqueria projects",
	"source": "./index.ts",
	"main": "./index.js",
	"bin": {
		"withTaq": "./bin/run.js"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc -p ./tsconfig.json && npx tsup"
	},
	"types": "./index.d.ts",
	"keywords": [
		"taqueria",
		"tezos",
		"pinnaclelabs",
		"ecadlabs"
	],
	"author": "Pinnacle Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/pinnacle-labs/taqueria.git",
		"directory": "taqueria-toolkit"
	},
	"bugs": {
		"url": "https://github.com/pinnacle-labs/taqueria/issues"
	},
	"homepage": "https://github.com/pinnacle-labs/taqueria#readme",
	"devDependencies": {
		"@types/cross-spawn": "^6.0.2",
		"@types/node": "^17.0.12",
		"@types/yargs": "^17.0.24",
		"tsup": "6.5.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@taqueria/protocol": "^0.28.7",
		"cross-spawn": "^7.0.3",
		"yargs": "^17.7.2"
	},
	"tsup": [
		{
			"entry": [
				"index.ts",
				"v1.ts",
				"v2.ts"
			],
			"sourcemap": true,
			"target": "es2018",
			"outDir": "./",
			"dts": true,
			"clean": false,
			"shims": true,
			"skipNodeModulesBundle": false,
			"platform": "browser"
		},
		{
			"entry": [
				"bin/run.ts"
			],
			"sourcemap": true,
			"target": "node16",
			"outDir": "./bin/",
			"dts": false,
			"clean": false,
			"skipNodeModulesBundle": true,
			"platform": "node"
		}
	],
	"exports": {
		"./lib/*": "./lib/*",
		".*": "./*",
		".": "./index.js",
		"./v1": "./v1.js",
		"./v2": "./v2.js"
	}
}
