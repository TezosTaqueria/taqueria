{
	"name": "@taqueria/toolkit",
	"version": "0.22.1",
	"description": "A TypeScript library for NodeJS to work with Taqueria projects",
	"main": "./index.js",
	"source": "./index.ts",
	"targets": {
		"main": {
			"context": "node",
			"isLibrary": true
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc -p ./tsconfig.json && npx esbuild --bundle ./index.ts --outfile=./index.js --minify --platform=node --sourcemap --target=es6"
	},
	"types": "./index.d.ts",
	"keywords": [
		"taqueria",
		"tezos",
		"ecad",
		"ecadlabs"
	],
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-toolkit"
	},
	"bugs": {
		"url": "https://github.com/ecadlabs/taqueria/issues"
	},
	"homepage": "https://github.com/ecadlabs/taqueria#readme",
	"devDependencies": {
		"@types/node": "^17.0.12",
		"esbuild": "^0.14.39",
		"typescript": "^4.7.2"
	},
	"dependencies": {}
}
