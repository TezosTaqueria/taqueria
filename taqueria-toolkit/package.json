{
	"name": "@taqueria/toolkit",
	"version": "0.62.8",
	"description": "A TypeScript library for NodeJS to work with Taqueria projects",
	"source": "./index.ts",
	"main": "./index.js",
	"bin": {
		"withTaq": "./bin/run.js"
	},
	"type": "module",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc -p ./tsconfig.json && npx tsup"
	},
	"types": "./index.d.ts",
	"keywords": [
		"taqueria",
		"tezos",
		"pinnaclelabs",
		"ecadlabs"
	],
	"author": "Taqueria",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/tezostaqueria/taqueria.git",
		"directory": "taqueria-toolkit"
	},
	"bugs": {
		"url": "https://github.com/tezostaqueria/taqueria/issues"
	},
	"homepage": "https://github.com/tezostaqueria/taqueria#readme",
	"devDependencies": {
		"@types/cross-spawn": "^6.0.6",
		"@types/node": "^20.10.4",
		"@types/yargs": "^17.0.32",
		"tsup": "^8.3.0",
		"typescript": "^5.6.2"
	},
	"dependencies": {
		"@taqueria/protocol": "workspace:*",
		"buffer": "^6.0.3",
		"cross-spawn": "^7.0.3",
		"yargs": "^17.7.2"
	},
	"tsup": [
		{
			"entry": [
				"index.ts",
				"v1.ts",
				"v2.ts"
			],
			"sourcemap": true,
			"target": [
				"esnext"
			],
			"outDir": "./",
			"dts": true,
			"clean": false,
			"shims": true,
			"skipNodeModulesBundle": false,
			"platform": "browser",
			"format": [
				"esm"
			]
		},
		{
			"entry": [
				"bin/run.ts"
			],
			"sourcemap": true,
			"target": "esnext",
			"outDir": "./bin/",
			"dts": false,
			"clean": false,
			"skipNodeModulesBundle": true,
			"platform": "node",
			"format": "esm"
		}
	],
	"exports": {
		"./lib/*": "./lib/*",
		".": "./index.js",
		"./v1": "./v1.js",
		"./v2": "./v2.js"
	},
	"gitHead": "ff58a2fc06ad233869ad6be574093c8b3b272e2e"
}
