{
	"name": "@taqueria/plugin-ipfs-pinata",
	"version": "0.28.2",
	"description": "A plugin for Taqueria providing ipfs publishing and pinning using the Pinata service",
	"keywords": [
		"taqueria",
		"plugin",
		"jest",
		"testing",
		"tdd",
		"ecad",
		"ecadlabs",
		"tezos"
	],
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule"
		}
	},
	"scripts": {
		"build": "npx tsc -noEmit -p ./tsconfig.json && npx tsup"
	},
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-plugin-ipfs-pinata"
	},
	"dependencies": {
		"@taqueria/node-sdk": "^0.28.2",
		"dotenv": "^16.0.0",
		"form-data": "^4.0.0",
		"ipfs-only-hash": "^4.0.0",
		"node-fetch": "^2.6.7"
	},
	"devDependencies": {
		"@types/node-fetch": "^2.6.1",
		"tsup": "^6.5.0",
		"typescript": "4.7.2"
	},
	"tsup": {
		"entry": [
			"index.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	}
}
