{
	"name": "@taqueria/plugin-ipfs-pinata",
	"version": "0.53.15",
	"description": "A plugin for Taqueria providing ipfs publishing and pinning using the Pinata service",
	"keywords": [
		"taqueria",
		"plugin",
		"jest",
		"testing",
		"tdd",
		"pinnaclelabs",
		"pinnacle-labs",
		"tezos"
	],
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule"
		}
	},
	"scripts": {
		"build": "npx tsc -noEmit -p ./tsconfig.json && npx tsup"
	},
	"author": "Pinnacle Labs",
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/pinnacle-labs/taqueria.git",
		"directory": "taqueria-plugin-ipfs-pinata"
	},
	"dependencies": {
		"@helia/unixfs": "^3.0.0",
		"@taqueria/node-sdk": "^0.53.15",
		"dotenv": "^16.4.1",
		"form-data": "^4.0.0",
		"helia": "^4.0.0",
		"node-fetch": "^3.3.2"
	},
	"devDependencies": {
		"@types/node-fetch": "^2.6.11",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs",
			"node": "./index.cjs",
			"default": "./index.js"
		},
		"./src/ipfsHash": {
			"import": "./src/ipfsHash.js",
			"require": "./src/ipfsHash.cjs",
			"node": "./src/ipfsHash.cjs",
			"default": "./src/ipfsHash.js"
		}
	},
	"tsup": {
		"entry": [
			"index.ts",
			"src/ipfsHash.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": false,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	},
	"gitHead": "ff58a2fc06ad233869ad6be574093c8b3b272e2e"
}
