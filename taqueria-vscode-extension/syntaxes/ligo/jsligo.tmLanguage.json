      "endCaptures": {},
      "patterns": [
        { "include": "#block_comment" }, { "include": "#attribute" }
      ]
    },
    "line_comment": {
      "name": "comment.block.jsligo",
      "begin": "\\/\\/",
      "end": "$",
      "beginCaptures": {},
      "endCaptures": {},
