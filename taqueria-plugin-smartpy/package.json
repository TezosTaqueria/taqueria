{
  "name": "@taqueria/plugin-smartpy",
  "version": "0.0.4",
  "description": "A taqueria plugin for compiling SmartPy smart contracts",
  "targets": {
    "default": {
      "source": "./index.ts",
      "distDir": "./",
      "context": "node",
      "isLibrary": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx parcel build --no-cache",
    "pluginInfo": "npx ts-node index.ts --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}"
  },
  "keywords": [
    "taqueria",
    "tezos",
    "build",
    "ecad",
    "ecadlabs",
    "plugin",
    "smartpy",
    "smart contract",
    "compile"
  ],
  "author": "ECAD Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecadlabs/taqueria.git",
    "directory": "taqueria-plugin-smartpy"
  },
  "bugs": {
    "url": "https://github.com/ecadlabs/taqueria/issues"
  },
  "homepage": "https://github.com/ecadlabs/taqueria#readme",
  "dependencies": {
    "fast-glob": "^3.2.7",
    "@taqueria/node-sdk": "^0.0.4"
  },
  "devDependencies": {
    "parcel": "^2.0.1",
    "typescript": "^4.5.2"
  }
}
