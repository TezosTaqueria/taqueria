{
	"name": "@taqueria/plugin-smartpy",
	"version": "0.13.0",
	"description": "A taqueria plugin for compiling SmartPy smart contracts",
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc -noEmit && npx parcel build --no-cache 2>&1",
		"pluginInfo": "npx ts-node index.ts --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}"
	},
	"keywords": [
		"taqueria",
		"tezos",
		"build",
		"ecad",
		"ecadlabs",
		"plugin",
		"smartpy",
		"smart contract",
		"compile"
	],
	"engines": {
		"node": ">=14.5"
	},
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-plugin-smartpy"
	},
	"bugs": {
		"url": "https://github.com/ecadlabs/taqueria/issues"
	},
	"homepage": "https://github.com/ecadlabs/taqueria#readme",
	"dependencies": {
		"@taqueria/node-sdk": "^0.13.0",
		"fast-glob": "^3.2.11"
	},
	"devDependencies": {
		"parcel": "2.6.1",
		"typescript": "^4.7.2"
	}
}
