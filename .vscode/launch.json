{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/taqueria-vscode-extension"
			],
			"outFiles": [
				"${workspaceFolder}/taqueria-vscode-extension/out/*.js"
			],
			"preLaunchTask": "build-and-watch-vscode",
			"env": {
				"LogLevel": "warn"
			}
		},
		{
			"request": "launch",
			"name": "Launch CLI",
			"type": "pwa-node",
			"program": "${workspaceFolder}/index.ts",
			"cwd": "/Users/jimmychenchen/Desktop/bugbug",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
				"--inspect-brk",
				"--allow-run",
				"--allow-write",
				"--allow-read",
				"--allow-env",
				"--allow-net",
				"--import-map",
				"${workspaceFolder}/import_map.json"
			],
			"args": [
				"--quickstart",
				"Dummy text",
				"--setBuild",
				"Dummy-build",
				"--setVersion",
				"Dummy-version",
				"uninstall",
				"../../taqueria/taqueria-plugin-flextesa"
			],
			"attachSimplePort": 9229
		}
	]
}
