{
	"name": "@taqueria/plugin-metadata",
	"version": "0.28.6",
	"description": "A plugin for Taqueria providing metadata creation and validation.",
	"keywords": [
		"taqueria",
		"plugin",
		"metadata",
		"pinnaclelabs",
		"pinnacle-labs",
		"tezos"
	],
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule"
		}
	},
	"scripts": {
		"build": "npx tsc -noEmit -p ./tsconfig.json && npx tsup"
	},
	"author": "Pinnacle Labs",
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/pinnacle-labs/taqueria.git",
		"directory": "taqueria-plugin-metadata"
	},
	"dependencies": {
		"@taqueria/node-sdk": "^0.28.6",
		"@taqueria/protocol": "^0.28.6",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"@types/prompts": "^2.4.4",
		"tsup": "^6.7.0",
		"typescript": "4.7.2"
	},
	"tsup": {
		"entry": [
			"index.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	}
}
