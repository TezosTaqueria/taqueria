{
	"name": "@taqueria/pipe",
	"version": "0.12.0",
	"main": "index.cjs",
	"module": "index.js",
	"source": "index.ts",
	"type": "module",
	"description": "A module of functions shared between both deno and node runtime",
	"keywords": [
		"taqueria",
		"ecad",
		"ecadlabs",
		"tezos"
	],
	"scripts": {
		"build": "npx tsc -noEmit -p ./tsconfig.json && npx tsup"
	},
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "index"
	},
	"devDependencies": {
		"typescript": "4.7.2"
	},
	"types": "./index.d.ts",
	"tsup": {
		"entry": [
			"index.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm"
		]
	},
	"dependencies": {
	}
}
