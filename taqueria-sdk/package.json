		"@types/project-name-generator": "^2.1.3",
		"@types/shell-quote": "^1.7.5",
		"@types/yargs": "^17.0.32",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"tsup": {
		"entry": [
			"index.ts",
			"types.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
