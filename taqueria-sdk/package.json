{
	"name": "@taqueria/node-sdk",
	"version": "0.80.1",
	"description": "A TypeScript SDK for NodeJS used for Taqueria plugin development.",
	"main": "./index.js",
	"source": "./index.ts",
	"module": "./index.mjs",
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./types": "./types.js"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"typecheck": "pnpm exec tsc -noEmit -p ./tsconfig.json",
		"build": "pnpm run clean && pnpm run typecheck && pnpm exec tsup",
		"clean": "rm -f index.js index.mjs index.d.ts index.js.map index.mjs.map"
	},
	"keywords": [
		"taqueria",
		"tezos",
		"build",
		"test",
		"deploy",
		"pinnaclelabs",
		"pinnacle-labs"
	],
	"author": "Taqueria",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/tezostaqueria/taqueria.git",
		"directory": "taqueria-sdk"
	},
	"bugs": {
		"url": "https://github.com/tezostaqueria/taqueria/issues"
	},
	"homepage": "https://github.com/tezostaqueria/taqueria#readme",
	"dependencies": {
		"@babel/runtime": "^7.28.4",
		"@taqueria/protocol": "workspace:*",
		"@taquito/signer": "24.0.0-RC.0",
		"@taquito/taquito": "24.0.0-RC.0",
		"@taquito/utils": "24.0.0-RC.0",
		"bip39": "^3.1.0",
		"bn.js": "^5.2.1",
		"fluture": "^14.0.0",
		"i18next": "^25.7.3",
		"node-fetch": "^3.3.2",
		"rambda": "^11.0.1",
		"shell-quote": "^1.8.3",
		"stacktrace-js": "^2.0.2",
		"ts-pattern": "^5.9.0",
		"why-is-node-running": "^3.2.2",
		"yargs": "^17.7.2",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@bevry/jsonfile": "^1.3.0",
		"@types/node": "^25.0.3",
		"@types/project-name-generator": "^2.1.3",
		"@types/shell-quote": "^1.7.5",
		"@types/yargs": "^17.0.35",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"tsup": {
		"entry": [
			"index.ts",
			"types.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	},
	"gitHead": "ff58a2fc06ad233869ad6be574093c8b3b272e2e"
}
