	"keywords": [
		"taqueria",
		"tezos",
		"build",
		"test",
		"deploy",
		"pinnaclelabs",
		"pinnacle-labs"
	],
	"author": "Pinnacle Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/pinnacle-labs/taqueria.git",
		"directory": "taqueria-sdk"
	},
	"bugs": {
		"url": "https://github.com/pinnacle-labs/taqueria/issues"
	},
	"homepage": "https://github.com/pinnacle-labs/taqueria#readme",
	"dependencies": {
		"@taqueria/protocol": "^0.48.0",
		"@taquito/signer": "^17.3.1",
		"@taquito/taquito": "^17.4.0",
		"@taquito/utils": "^17.4.0",
		"i18next": "^23.7.8",
		"node-fetch": "^3.3.2",
		"rambda": "^8.6.0",
		"shell-quote": "^1.8.1",
		"stacktrace-js": "^2.0.2",
		"ts-pattern": "^5.0.6",
