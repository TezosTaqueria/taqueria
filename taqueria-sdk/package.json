{
  "name": "@taqueria/node-sdk",
  "version": "0.4.1",
  "description": "A TypeScript SDK for NodeJS used for Taqueria plugin development.",
  "main": "./index.js",
  "source": "./index.ts",
  "targets": {
    "main": {
      "context": "node",
      "isLibrary": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc -noEmit && npx esbuild --bundle index.ts types.ts --outdir=. --minify --platform=node --sourcemap"
  },
  "keywords": [
    "taqueria",
    "tezos",
    "build",
    "test",
    "deploy",
    "ecad",
    "ecadlabs"
  ],
  "author": "ECAD Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecadlabs/taqueria.git",
    "directory": "taqueria-sdk"
  },
  "bugs": {
    "url": "https://github.com/ecadlabs/taqueria/issues"
  },
  "homepage": "https://github.com/ecadlabs/taqueria#readme",
  "dependencies": {
    "@taqueria/protocol": "^0.4.1",
    "i18next": "^21.8.4",
    "project-name-generator": "^2.1.9",
    "rambda": "^7.1.4",
    "stack-trace": "github:mweichert/node-stack-trace",
    "ts-pattern": "^4.0.2",
    "yargs": "^17.2.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@bevry/jsonfile": "^1.3.0",
    "@types/json-schema-generator": "^2.0.0",
    "@types/node": "^17.0.12",
    "@types/project-name-generator": "^2.1.1",
    "@types/stack-trace": "^0.0.29",
    "@types/yargs": "^17.0.7",
    "esbuild": "^0.14.39",
    "typescript": "^4.7.2"
  }
}
