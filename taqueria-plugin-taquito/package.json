{
	"name": "@taqueria/plugin-taquito",
	"version": "0.28.4",
	"description": "A taqueria plugin for originating smart contracts using Taquito",
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"pluginInfo": "npx ts-node index.ts --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}",
		"build": "npx tsc -noEmit -p ./tsconfig.json && npx tsup",
		"debugPluginInfo": "node --inspect-brk index.js --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}"
	},
	"keywords": [
		"taqueria",
		"tezos",
		"originate",
		"deploy",
		"ecad",
		"ecadlabs",
		"plugin",
		"taquito",
		"smart contract"
	],
	"engines": {
		"node": ">=16"
	},
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-plugin-taquito"
	},
	"bugs": {
		"url": "https://github.com/ecadlabs/taqueria/issues"
	},
	"homepage": "https://github.com/ecadlabs/taqueria#readme",
	"dependencies": {
		"@taqueria/node-sdk": "^0.28.4",
		"@taquito/michel-codec": "^16.0.1",
		"@taquito/signer": "^16.0.1",
		"@taquito/taquito": "^16.0.1",
		"fast-glob": "^3.2.11"
	},
	"devDependencies": {
		"tsup": "^6.5.0",
		"typescript": "^4.7.2"
	},
	"tsup": {
		"entry": [
			"index.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": true,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	}
}
