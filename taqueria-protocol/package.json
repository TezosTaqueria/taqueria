{
	"name": "@taqueria/protocol",
	"version": "0.19.1",
	"description": "A TypeScript package which contains types that are to be shared between @taqueria/node-sdk and @taqueria/taqueria.",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tsc --emitDeclarationOnly -p ./tsconfig.json && npx tsup"
	},
	"keywords": [
		"taqueria",
		"tezos",
		"build",
		"test",
		"deploy",
		"ecad",
		"ecadlabs",
		"protocol"
	],
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-protocol"
	},
	"bugs": {
		"url": "https://github.com/ecadlabs/taqueria/issues"
	},
	"homepage": "https://github.com/ecadlabs/taqueria#readme",
	"devDependencies": {
		"typescript": "^4.7.2"
	},
	"exports": {
		"./*": {
			"import": "./*.mjs",
			"default": "./*.js"
		},
		"./*.js": {
			"import": "./*.mjs",
			"default": "./*.js"
		}
	},
	"dependencies": {
		"@peculiar/webcrypto": "^1.4.0",
		"batching-toposort": "^1.2.0",
		"fluture": "^14.0.0",
		"rambda": "^7.2.1",
		"rambdax": "^8.1.0",
		"tsup": "^6.2.3",
		"zod": "^3.17.3"
	},
	"tsup": {
		"entry": [
			"[!.]+.ts"
		],
		"sourcemap": true,
		"target": "node16",
		"outDir": "./",
		"dts": false,
		"clean": false,
		"skipNodeModulesBundle": true,
		"platform": "node",
		"format": [
			"esm",
			"cjs"
		]
	}
}
