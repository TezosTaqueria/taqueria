{
	"name": "@taqueria/plugin-core",
	"version": "0.24.2",
	"description": "A taqueria plugin for core tasks",
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"pluginInfo": "npx ts-node index.ts --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}",
		"build": "npx tsc -noEmit && npx parcel build --no-cache 2>&1",
		"debugPluginInfo": "node --inspect-brk index.js --taqRun pluginInfo --i18n {\"foo:\"\"bar\"}"
	},
	"keywords": [
		"taqueria",
		"tezos",
		"ecad",
		"ecadlabs",
		"plugin",
		"core"
	],
	"engines": {
		"node": ">=16"
	},
	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-plugin-core"
	},
	"bugs": {
		"url": "https://github.com/ecadlabs/taqueria/issues"
	},
	"homepage": "https://github.com/ecadlabs/taqueria#readme",
	"devDependencies": {
		"parcel": "2.6.1",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"@taqueria/node-sdk": "^0.22.2"
	}
}
